<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit trunk//EN" "http://google-web-toolkit.googlecode.com/svn/trunk/distro-source/core/src/gwt-module.dtd">
<module rename-to="countries">

    <inherits name="com.badlogic.gdx.backends.gdx_backends_gwt" />
    <inherits name="es.eucm.i18n"/>
    <inherits name="es.eucm.gleaner.TraceViewer"/>
    <entry-point class="com.anserran.countries.CountriesHtml" />

    <source path="countries" />
    <set-configuration-property name="gdx.assetpath" value="assets" />
    <set-configuration-property name="gdx.assetoutputpath" value="html/target/webapp" />
    <extend-configuration-property name="gdx.reflect.include" value="com.anserran.countries.view.DecreasingBar" />
</module>